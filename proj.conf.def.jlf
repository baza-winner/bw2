${
  bwDir String
}

$conf = %\($bwgDir)/data/conf.jlf
$ports{_conf Map} = $_conf|ports
$portRange = $conf|portRange|^Range

^{
  type Map
  keysDef {
    bw {
      type Map
      keysDef {
        tag String
      }
    }
    projPortIncrement ^{
      type Int
      range 0..($portRange|max - $portRange|min)
    }
    ports ^{
      type Map
      keys $conf|$ports
      elemDef Map
    }
  }
}
