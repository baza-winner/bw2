$portRange = %\($bw\hgDir)/data/conf.jlf | r\( *\12)ange | ^{ type Int }

^{
  type Map
  keysDef {
    bw {
      type Map
      keysDef {
        tag String
      }
    }
    projPortIncrement {
      type Int
      range (*Int)..90
      range 0..($portRange|max - $portRange|min)
    }
    ports {
      type Map
      elemDef Map
    }
  }
}