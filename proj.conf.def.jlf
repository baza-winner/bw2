!conf = (%\($bw\hgDir)/data/conf.jlf)
!portRange = ($conf|portRange^Range ?)

^{
  type Map
  keysDef {
    bw {
      type Map
      keysDef {
        tag String
      }
    }
    projPortIncrement ^{
      type Int
      range 0..(($portRange|max?) - ($portRange|min))
    }
    ports ^{
      type Map
      keys ( $conf|ports )
      elemDef Map
    }
  }
}
